#if (! defined(YPPCONFIG_H_))
# define YPPCONFIG_H_ /* */

/*! @brief The major part of the version number. */
# define YPP_VERSION_MAJOR @YPP_VERSION_MAJOR@
/*! @brief The minor part of the version number. */
